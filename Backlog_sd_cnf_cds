@AbapCatalog.sqlViewName: 'ZAEU_CNFQ'
@AbapCatalog.compiler.compareFilter: true
@AbapCatalog.preserveKey: true
@AccessControl.authorizationCheck: #NOT_REQUIRED
@EndUserText.label: 'Helper CDS view to get Confirmed Quantities'
@Metadata.ignorePropagatedAnnotations: true
define view ZAEU_CNFQ_CDS as select from vbep
{
  vbeln,
  posnr,
  lifsp,
  sum( bmeng ) as conf_qty,
  min( edatu ) as firstreqdt
}
group by vbeln, posnr , lifsp
